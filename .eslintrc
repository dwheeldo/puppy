{
  "extends": [
    "./node_modules/eslint-config-airbnb-base/.eslintrc",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100
      }
    ]
  },
  "plugins": [
    "prettier"
  ]
}
