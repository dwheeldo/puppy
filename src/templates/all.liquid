    <head>
        <title>Wild Dog Design</title>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" id="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, minimal-ui">
        <meta name="robots" content="noindex, nofollow">

    <style>
      @import url(http://fonts.googleapis.com/css?family=Ubuntu:500,400,300);

      html{
        -webkit-text-size-adjust: none;
        zoom: 1;
        height: 100%;
      }
      body{
        font: 300 13px/1.75 'Ubuntu', Tahoma, Geneva, sans-serif;
        color: #58585a;
        height: 100%;
        min-width: 250px;
        padding: 0;
        margin: 0;
      }

      p{
        margin: 0 0 10px 0;
      }

      .container{
        display: block;
        width: 940px;
        margin: 0 auto;
      }
      @media (max-width: 979px) {
        .container{
          width: auto;
          margin: 0 20px;
        }
      }

      .container:before,
      .container:after {
        display: table;
        line-height: 0;
        content: "";
      }
      .container:after {
        clear: both;
      }


      header{
        border-bottom: 1px solid #abacad;
        margin-left: -20px;
        margin-right: -20px;
        padding-left: 20px;
        padding-right: 20px;
      }
        header .logo,
        header .logo a{
          display: block;
          width: 332px;
          height: 47px;
        }
        header .logo{
          margin: 16px 0 13px 0;
        }
          header .logo a{
            background: 0 0 no-repeat;
            background-image: url(data:image/png;base64,);
            overflow: hidden;
            text-indent: -999px;
            font-size: 1px;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
          }
          @media screen and (-webkit-min-device-pixel-ratio: 2), screen and (max--moz-device-pixel-ratio: 2), screen and (min-device-pixel-ratio: 2) {
            header .logo a{
              background-image: url(data:image/png;base64,);
            }
          }

          @media (max-width: 767px) {
            header .logo,
            header .logo a{
              width: 280px;
              height: 40px;
            }

            header .logo{
              margin: 20px 0;
            }
          }
          @media (max-width: 480px) {
            header .logo,
            header .logo a{
              width: 75px;
            }
            header .logo{
              margin: 8px 0 10px 0;
            }
          }

      article{}
        article hr{
          margin: 40px auto 0 auto;
          width: 75%;
          border: 0 none;
          border-bottom: 1px solid #dee0e1;
        }
        article h2{
          font-weight: 400;
          font-size: 16px;
          line-height: 1.3;
        }
          article h2 span{
            font-weight: 300;
            font-size: 14px;
            margin-left: 15px;
          }
          @media (max-width: 660px) {
            article h2 span{
              margin-left: 0;
              display: block;
            }
          }
        article h3{
          font-weight: 400;
          margin: 40px 0 0 0;
          font-size: 14px;
        }
        article .link-list{
          list-style: none;
          border-left: 1px solid #dee0e1;
          margin: 5px 0 0 5px;
          padding: 0 0 0 10px;
        }
          article .link-list li{
            padding: 0 0 20px 0;
            line-height: 1.2;
          }
            article .link-list li strong,
            article .link-list li b{
              font-weight: 500;
            }
          article .link-list li:last-child{
            padding-bottom: 0;
          }
            article .link-list li a{
              font-weight: 400;
              color:  #D90000;
              text-decoration: none;
            }
            article .link-list li a:hover{
              color: #58585a;
            }
            article .link-list li span{
              display: block;
              font-size: 11px;
            }
            article .link-list li .download{
              padding-right: 18px;
              background: url(data:image/png;base64,) 100% 25% no-repeat;
            }
            article .link-list li .download:hover{
              background-image: url(data:image/png;base64,);
            }

          article .link-list .no-link{
            opacity: 0.5;
          }
            article .link-list .no-link a{
              cursor: not-allowed;
              text-decoration: line-through;
              color: #58585a;
            }
              article .link-list .no-link a span{
                color: #d90000;
                font-size: 13px;
              }
      footer{
        border-top: 1px solid #abacad;
        padding: 15px 0;
        margin-top: 75px;
        margin-left: -20px;
        margin-right: -20px;
        padding-left: 20px;
        padding-right: 20px;
      }
        footer .last-modified{
          margin: 0 0 20px 0;
          font-weight: 400;
        }
        footer p{
          margin: 0;
          line-height: 1.2;
        }
          footer p br{
            display: none;
          }
          @media (max-width: 660px) {
            footer p br{
              display: block;
            }
          }

    </style>

    <link rel="icon" type="image/png" href="favicon.png">
    <link rel="apple-touch-icon" href="apple-touch-icon.png">
        <!--[if lte IE 8]>
            <link rel="icon" type="image/x-icon" href="favicon.ico">
        <![endif]-->

    <script src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.6.2/modernizr.min.js"></script>
    </head>
    <body>
    <div class="container">
      <header>
        <h1 class="logo">
          <a href="http://wilddogdesign.co.uk/">Wild Dog Design</a>
        </h1>
      </header>
      <article>
        <h2>{{ pages.config.title }} <span>Static/Front-end website development</span></h2>

        <h3>Page templates</h3>
        <ol class="link-list">
        {% for p in pages.all %}
          <li>
            <a href="{{ p.page }}.html">{{ p.title }}</a>
            {% if p.description %}<span>- {{ p.description }}</span>{% endif %}
          </li>
        {% endfor %}
        </ol>

        <h3>Styleguide</h3>
        <ol class="link-list">
          <li>
            <a href="styleguide/">{{ pages.config.title }} Styleguide</a>
            <span>- Your living styleguide</span>
          </li>
        </ol>

      </article>
      <footer>
        <p class="last-modified">Last modified: {{ dateNow }}</p>
        <p>Wild Dog Design, <br>44-46 Old Steine, Brighton, <br>East Sussex. BN1 1NH. <br>United Kingdom</p>
        <p>&copy; Wild Dog Design {{ yearNow }}</p>
      </footer>
    </div>

    </body>
</html>
