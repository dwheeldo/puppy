      article{}
        article hr{
          margin: 40px auto 0 auto;
          width: 75%;
          border: 0 none;
          border-bottom: 1px solid #dee0e1;
        }
        article h2{
          font-weight: 400;
          font-size: 16px;
          line-height: 1.3;
        }
          article h2 span{
            font-weight: 300;
            font-size: 14px;
            margin-left: 15px;
          }
          @media (max-width: 660px) {
            article h2 span{
              margin-left: 0;
              display: block;
            }
          }
        article h3{
          font-weight: 400;
          margin: 40px 0 0 0;
          font-size: 14px;
        }
        article .link-list{
          list-style: none;
          border-left: 1px solid #dee0e1;
          margin: 5px 0 0 5px;
          padding: 0 0 0 10px;
        }
          article .link-list li{
            padding: 0 0 20px 0;
            line-height: 1.2;
          }
            article .link-list li strong,
            article .link-list li b{
              font-weight: 500;
            }
          article .link-list li:last-child{
            padding-bottom: 0;
          }
            article .link-list li a{
              font-weight: 400;
              color:  #D90000;
              text-decoration: none;
            }
            article .link-list li a:hover{
              color: #58585a;
            }
            article .link-list li span{
              display: block;
              font-size: 11px;
            }
          article .link-list .no-link{
            opacity: 0.5;
          }
            article .link-list .no-link a{
              cursor: not-allowed;
              text-decoration: line-through;
              color: #58585a;
            }
              article .link-list .no-link a span{
                color: #d90000;
                font-size: 13px;
              }
      footer{
        border-top: 1px solid #abacad;
        padding: 15px 0;
        margin-top: 75px;
        margin-left: -20px;
        margin-right: -20px;
        padding-left: 20px;
        padding-right: 20px;
      }
        footer .last-modified{
          margin: 0 0 20px 0;
          font-weight: 400;
        }
        footer p{
          margin: 0;
          line-height: 1.2;
        }
          footer p br{
            display: none;
          }
          @media (max-width: 660px) {
            footer p br{
              display: block;
            }
          }
