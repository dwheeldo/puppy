{
  "name": "Puppy",
  "version": "2.0.0",
  "description": "Woof!",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilddogdesign/puppy.git"
  },
  "scripts": {
    "build": "gulp build:production",
    "deploy": "fly deploy:development",
    "empty-cache": "gulp cache:clear",
    "serve": "gulp serve",
    "setup": "yarn install && jspm install && bower install",
    "start": "gulp",
    "styleguide": "gulp styleguide",
    "update-deps": "yarn upgrade && jspm install && bower install && bower prune"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "settings": {
      "import/resolver": {
        "jspm": {
          "moduleDirectory": "jspm_packages"
        },
        "node": true
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "browser-sync": "^2.12.5",
    "cssnano": "^3.5.2",
    "del": "^2.2.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-jspm": "^2.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "flightplan": "^0.6.13",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-cache": "^0.4.3",
    "gulp-data": "^1.2.1",
    "gulp-eslint": "^3.0.1",
    "gulp-favicons": "^2.2.6",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-inject": "^4.0.0",
    "gulp-jsbeautifier": "^2.0.2",
    "gulp-jspm": "^0.5.7",
    "gulp-modernizr": "^1.0.0-alpha",
    "gulp-nunjucks-render": "^2.0.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-styledown": "0.0.2",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.5",
    "imagemin-jpeg-recompress": "^5.1.0",
    "jspm": "^0.16.30",
    "moment": "^2.13.0",
    "nunjucks-date-filter": "^0.1.1",
    "path": "^0.12.7",
    "postcss-critical-split": "^2.4.1",
    "postcss-inline-svg": "^2.0.1"
  },
  "jspm": {
    "dependencies": {
      "domready": "npm:domready@^1.0.8",
      "svg4everybody": "npm:svg4everybody@^2.0.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
