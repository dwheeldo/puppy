{
  "name": "Puppy",
  "version": "3.1.0",
  "description": "Woof!",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilddogdesign/puppy.git"
  },
  "scripts": {
    "build": "gulp build:production",
    "deploy": "fly deploy:development",
    "empty-cache": "gulp cache:clear",
    "serve": "gulp serve",
    "setup": "yarn install && jspm install",
    "start": "gulp",
    "styleguide": "gulp styleguide",
    "update-deps": "yarn upgrade && jspm install"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "settings": {
      "import/resolver": {
        "jspm": {
          "moduleDirectory": "jspm_packages"
        },
        "node": true
      }
    }
  },
  "dependencies": {
    "gulp-sass-lint": "^1.3.2",
    "inuitcss": "^6.0.0-beta.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "browser-sync": "^2.18.13",
    "cssnano": "^3.5.2",
    "del": "^3.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-jspm": "^2.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "flightplan": "^0.6.13",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-cache": "^1.0.1",
    "gulp-data": "^1.2.1",
    "gulp-eslint": "^4.0.0",
    "gulp-favicons": "^2.2.6",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.4.0",
    "gulp-inject": "^4.3.0",
    "gulp-jsbeautifier": "^2.0.2",
    "gulp-jspm": "^0.5.7",
    "gulp-modernizr": "^1.0.0-alpha",
    "gulp-nunjucks-render": "^2.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^8.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-shell": "^0.6.3",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-styledown": "0.0.2",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^4.3.5",
    "imagemin-jpeg-recompress": "^5.1.0",
    "jspm": "^0.16.30",
    "moment": "^2.13.0",
    "nunjucks-date-filter": "^0.1.1",
    "path": "^0.12.7",
    "postcss-critical-split": "^2.4.1",
    "postcss-inline-svg": "^3.0.0"
  },
  "jspm": {
    "dependencies": {
      "domready": "npm:domready@^1.0.8",
      "svg4everybody": "npm:svg4everybody@^2.1.9"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^6.26.0",
      "babel-runtime": "npm:babel-runtime@^6.26.0",
      "core-js": "npm:core-js@^2.5.1"
    }
  }
}
